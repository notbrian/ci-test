{
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "allowImportExportEverywhere": true
    },
    "plugins": [
        "meteor"
    ],
    "extends": [
        "airbnb",
        "plugin:meteor/recommended"
    ],
    "settings": {
        "import/resolver": "meteor"

    },
    "rules": {
      "import/extensions": ["off", "never"],
      "import/no-extraneous-dependencies": "off"
    }
}
